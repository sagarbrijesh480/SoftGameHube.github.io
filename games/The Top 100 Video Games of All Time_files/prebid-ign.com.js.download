function getZadDivs(){
	var zadElement = document.querySelectorAll('[id^="zad-"]');

	zadElement.forEach((element,index)=>{  
        var elementID = element.id;
        var zadPElementSplot = elementID.split("-");
        var zadPElementName = zadPElementSplot[1];
        // console.log(zadPElementName);
        switch(zadPElementName){
        	case 'billboard':
        	case 'mrec':
        	case 'halfpage':
        	case 'footer':
        		zadAds[zadPElementName]["divId"] = elementID;
        		cadsAllAdsSlots[elementID] = zadAds[zadPElementName];
        		cadsAllAdsSlotsBidder[elementID] = zadBidder[zadPElementName];
        		if(zadAds[zadPElementName].aps){              
                	fmgRequestedUnitAPS.push({ slotID: elementID,slotName: zadAds[zadPElementName].adCode, sizes:zadAds[zadPElementName].size});
	            }

	            if(zadAds[zadPElementName].prebid){   
	                fmgRequestedUnit.push({ code: elementID, mediaTypes: { banner: { sizes: zadAds[zadPElementName].size } }, bids: cadsAllAdsSlotsBidder[elementID] });
	            } 
        		// console.log(cadsAllAdsSlots);
        	break;

        }
        

    }); 
}
getZadDivs();

function fmgMsg(msgType="log",msg){
	if(msgType=="log"){
		console.log("%cFMGMsg: "+msg,'color: #bada55');	
	}
	if(msgType=="warning"){
		console.log("%cFMGMsg: "+msg,'color: #f00');	
	}
}


function sendAdserverRequest(){
	if(pbjs.adserverRequestSent) return;
    pbjs.adserverRequestSent = true;
    googletag.cmd.push(function(){
        pbjs.que.push(function(){

            pbjs.setTargetingForGPTAsync();

            for(let pair of queryString.entries()) {
		    	googletag.pubads().setTargeting(pair[0],pair[1]);
		    }


		    if(document.referrer!=""){
		    	googletag.pubads().setTargeting("ref",document.referrer);
		    }
		    if(window.location.pathname!=""){
		    	googletag.pubads().setTargeting("locationpath",window.location.pathname);
		    }

		    if(window.$cads_category !== undefined){		    	
		    	googletag.pubads().setTargeting("cads_category",window.$cads_category);
		    }

		    if(window.$cads_keywords !== undefined){		    	
		    	googletag.pubads().setTargeting("cads_keywords",window.$cads_keywords);
		    }

		    if(window.$cads_bsafe !== undefined){		    	
		    	googletag.pubads().setTargeting("cads_bsafe","Yes");
		    }
		    
		    googletag.pubads().disableInitialLoad();

	        googletag.pubads().enableLazyLoad({
	          fetchMarginPercent: -1,  //fetch on load
	          renderMarginPercent: 300, // render ads before ads coming to viewable port. higher number will load ads early
	          mobileScaling: 2.0
	        }); 

		    googletag.pubads().addEventListener("impressionViewable", impressionViewabled);


		    // Enable SRA and services.
	        // googletag.pubads().enableSingleRequest();
	        googletag.enableServices();

		    // // Register event handlers to observe lazy loading behavior.
	        // googletag.pubads().addEventListener("slotRequested", (event) => {
	        //   console.log(event.slot.getSlotElementId(), " fetched");
	        // });

	        // googletag.pubads().addEventListener("slotOnload", (event) => {
	        //   console.log(event.slot.getSlotElementId(), " rendered");
	        // });

	        // Refresh all adslots.
		    // googletag.pubads().refresh();
		    
	    	googletag
	                .pubads()
	                .getSlots()
	                .forEach(function (slot) {
	                    if(slot.getSlotElementId().includes("cadsAuction-") || slot.getSlotElementId().includes("zad-")){ 
	                    	googletag.pubads().refresh([slot]); 
	                    	fmgMsg("log","Unit :  "+slot.getSlotElementId());
	                    }else{
	                    	fmgMsg("warning","unit not from fmgAuction or zad : "+slot.getSlotElementId());	
	                    }
	                });                  
        })
    })
}

function readOnLoadedContainer(){

    var auctionElement = document.querySelectorAll('[id^="cadsAuction-"]');
    
    googletag.cmd.push(function() {

	    auctionElement.forEach((element,index)=>{  
	        var item = element.id;
	        if(cadsAllAdsSlots[item]){
		    	fmgMsg("log","unit creation start for "+item);
		        var selectedDiv = cadsAllAdsSlots[item];
		            selectedSlot = googletag.defineSlot(selectedDiv.adCode, selectedDiv.size, `${item}`)
		                            .setCollapseEmptyDiv(true)
		                            .addService(googletag.pubads());

		            if(selectedDiv.aps){              
		                fmgRequestedUnitAPS.push({ slotID: item,slotName: selectedDiv.adCode, sizes:selectedDiv.size});
		            }

		            if(selectedDiv.prebid){   
		                fmgRequestedUnit.push({ code: item, mediaTypes: { banner: { sizes: selectedDiv.size } }, bids: cadsAllAdsSlotsBidder[item] });
		            } 
		    }else{
		    	fmgMsg("warning","config is not defined "+item);
		    }
	    }); 
    }); 
    
    if(fmgRequestedUnitAPS.length>0){
    	// console.log("APS Auction for ",fmgRequestedUnitAPS);
	    apstag.fetchBids({
	        slots:fmgRequestedUnitAPS	        
	    }, function(bids) {
	        apstag.setDisplayBids();
	    }); 
	}

    if(fmgRequestedUnit.length>0){
    	// console.log("Prebid Auction for ",fmgRequestedUnit);

    	pbjs.bidderSettings = { 
		    "ix": { 
		        "storageAllowed": true 
		    },
		    "amx": {
			    "storageAllowed": true,
			},  
		};

    	pbjs.setConfig({
    		"userSync": {
    			"iframeEnabled": true,
		        "filterSettings": {
		            "iframe": {
		                "bidders": '*',   // '*' means all bidders
		                "filter": 'include'
		            }
		        }
		    },
		  	"schain": {
			    "validation": "relaxed",
			    "config": {
			      "ver":"1.0",
			      "complete": 1,
			      "nodes": [
			        {
			          "asi":"datacygnal.io",
			          "sid":"200957",
			          "hp":1
			        }
			      ]
			    }
		  	}
		});

		
    	pbjs.que.push(function(){
    		pbjs.addAdUnits(fmgRequestedUnit);
        	pbjs.requestBids({ bidsBackHandler: sendAdserverRequest });	
    	});
    }

    setTimeout(function(){
            sendAdserverRequest();
    }, auctionTimeout);   
}

readOnLoadedContainer();

