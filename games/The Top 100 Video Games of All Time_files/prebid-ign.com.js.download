    console.log("slotRendered",event.slot.getSlotElementId());
}

function impressionViewabled(event){
    var slot = event.slot.getSlotElementId();
    if(cadsAllAdsSlots[slot] && cadsAllAdsSlots[slot]['autoRefresh']){
        if(cadsAllAdsSlots[slot]["autoRefreshCounter"] < cadsAllAdsSlots[slot]["autoRefreshMaxLimit"]){
            setTimeout(function() {
                cadsAllAdsSlots[slot]["autoRefreshCounter"] = cadsAllAdsSlots[slot]["autoRefreshCounter"] + 1;
                fmgMsg("log","auto refresh for "+slot);
                // fmgMsg("log",event.slot.getSlotElementId()+" refresh after "+cadsAllAdsSlots[slot]["autoRefreshTime"]+" sec");
                googletag.pubads().refresh([event.slot]);
            }, 1E3 * cadsAllAdsSlots[slot]["autoRefreshTime"]);    
        }else{
            fmgMsg("log","Max refresh limit reach for "+slot);
        }   
    }    
}